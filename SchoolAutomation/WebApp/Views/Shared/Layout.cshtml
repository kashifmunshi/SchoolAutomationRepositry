<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="viewport" content="width=device-width"/>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("/css/bootstrap.min.css")" rel="stylesheet"/>
    <link href="@Url.Content("/css/main.css")" rel="stylesheet"/>
    <script src="@Url.Content("/js/jquery.min.js")"></script>
    <script src="@Url.Content("/js/bootstrap.min.js")"></script>
    <script src="@Url.Content("/js/main.js")"></script>
</head>
<body>

<div class="container mt-3">
    <div class="row">
        @RenderBody()
    </div>
    <footer class="pt-5 d-flex justify-content-between">
        <span>Copyright © @DateTime.Now.Year Scool Automation</span>
    </footer>
</div>
@RenderBody()
</body>
</html>